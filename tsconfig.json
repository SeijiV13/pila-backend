{
   "compilerOptions": {
       "typeRoots": [
			"./node_modules/@types",
         "./node_modules/koa-body"
	  ],
      "lib": [
         "ES5",
         "ES6",
         "ESNext",
         "ES7"
      ],
      "target": "ES5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true
   } , 
   "exclude": [
      "node_modules"
   ]

}